<!-- LiftOvers Pick-Up -->
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 style="margin: 50px 0 100px 0;">Pick-Ups</h1>
            </div>
        </div>

      <div class="row">
        <div class="col-lg-12">

        <!-- Pick-Up Schedule -->
          <div class="panel panel-primary">
            <div class="panel-heading">
              <h3 class="panel-title"><strong>Current Available Pick-Ups</strong></h3>
            </div>
            <div class="panel-body">
              <table class="table table-hover" id="pick-up-table">
                <thead>
                  <tr>
                    <th>Organization Name</th>
                    <th>Contact</th>
                    <th>Phone</th>
                    <th>Location</th>
                  </tr>
                  <tr>
                      <td>Test Organization Name</td>
                      <td>Test Name</td>
                      <td>(888) 888-8888</td>
                      <td>888 Main St., CA, 99999-9999</td>
                  </tr>
                  <tr>
                      <td>Test Organization Name</td>
                      <td>Test Name</td>
                      <td>(888) 888-8888</td>
                      <td>888 Main St., CA, 99999-9999</td>
                  </tr>
                  <tr>
                      <td>Test Organization Name</td>
                      <td>Test Name</td>
                      <td>(888) 888-8888</td>
                      <td>888 Main St., CA, 99999-9999</td>
                  </tr>
                </thead>
                <tbody>

                </tbody>
              </table>
            </div>
          </div>

          <!-- Add Pick-Up -->
          <div class="panel panel-primary">
            <div class="panel-heading">
              <h3 class="panel-title" style="margin-top: 100px;"><strong>Add Pick-Up</strong></h3>
            </div>
            <div class="panel-body">

              <!-- Entry Form -->
              <form>
                <div class="form-group">
                  <label for="organization-name-input">Orgnization Name</label>
                  <input type="text" class="form-control" id="organization-name-input">
                </div>
                <div class="form-group">
                  <label for="location-input">Location</label>
                  <input type="text" class="form-control" id="location-input">
                </div>
                <div class="form-group">
                  <label for="pick-up-before-input">Pick Up Before (HH:mm - military time)</label>
                  <input type="text" class="form-control" id="pick-up-before-input">
                </div>
                <button type="submit" class="btn btn-primary" id="add-pick-up-btn">Submit</button>
              </form>
              <p>&nbsp;</p><p>&nbsp;</p>
            </div>
          </div>
        </div>
      </div>
    </div>